<%- include('header.ejs') %>

<body>
    <script src="https://cdn.plot.ly/plotly-2.4.2.min.js"></script>
    <div id="tester" style="width:600px;height:250px;"></div>

    <script>
        TESTER = document.getElementById('tester');
        const x1 = "<%= x1 %>".split(',')
        const y1 = "<%= y1 %>".split(',')
        var d = [{
            values: y1,
            labels: x1,
            type: 'pie'
        }]
        var layout = {
            height: 400,
            width: 500
        };
        Plotly.newPlot(TESTER, d, layout);
    </script>
</body>

</html>