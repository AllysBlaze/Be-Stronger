<%- include('header.ejs') %>

<body>
    <%- include('sideBarMenu.ejs') %>

    <div id="main">
        <button class="btn btn-outline-dark openbtn" onclick="openNav()">&#9776; Menu</button>
        <div class="col-lg-9 mx-auto">
            <table class="table table-dark table-striped">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Ćwiczenie</th>
                        <th scope="col">Ilość powtórzeń</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(var i=0;i<names.length;i++){ %>
                    <tr>
                        <th scope="row"><%= i+1 %></th>
                        <td><%= names[i] %></td>
                        <td><%= rep[i] %></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
        <a href="/home/sets/start?id=<%= set_id %>" class="btn btn-warning" value="<%= set_id[i] %>">Rozpocznij</a>
    </div>
    <%- include('footer.ejs') %>

</body>

</html>